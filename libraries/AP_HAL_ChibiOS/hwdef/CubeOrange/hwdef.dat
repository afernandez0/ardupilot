# hw definition file for processing by chibios_hwdef.py

# MCU class and specific type
MCU STM32H7xx STM32H743xx

include hwdef.inc

# build ABIN for flash-from-bootloader support:
env BUILD_ABIN True

# ajfg
define AP_CHECK_FIRMWARE_ENABLED      1

# Root folder 'build/board_name'
env AP_FIRMWARE_CHECKSUM_KEY         firmware.chksum
env AP_FIRMWARE_CHECKSUM_FILE        bin/arducopter.chksum

env AP_PARAMETERS_CHECKSUM_KEY       defaults.chksum
env AP_PARAMETERS_CHECKSUM_FILE      processed_defaults.chksum